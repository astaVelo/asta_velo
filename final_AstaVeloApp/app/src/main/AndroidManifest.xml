<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.raywenderlich.asta_velo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name="com.raywenderlich.asta_velo.RentBike" />
        <activity android:name="com.raywenderlich.asta_velo.MainActivity" />
        <activity android:name="com.raywenderlich.asta_velo.Finish" />
        <activity android:name="com.raywenderlich.asta_velo.Appointment" />
        <activity
            android:name="com.raywenderlich.asta_velo.Start"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.raywenderlich.asta_velo.PopActivity" android:theme="@style/AppTheme.PopMe"></activity>
    </application>

</manifest>